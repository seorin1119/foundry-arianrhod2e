{
  "id": "arianrhod2e",
  "title": "Arianrhod RPG 2E",
  "description": "アリアンロッドRPG 2E - A JRPG-style fantasy TRPG system by F.E.A.R. for Foundry Virtual Tabletop.",
  "version": "0.6.0",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "FVTT-AR2 Contributors"
    }
  ],
  "esmodules": ["arianrhod2e.mjs"],
  "styles": ["styles/arianrhod2e.css"],
  "packs": [
    {
      "name": "skills",
      "label": "Skills / スキル",
      "path": "packs/skills",
      "type": "Item",
      "system": "arianrhod2e"
    },
    {
      "name": "equipment",
      "label": "Equipment / 装備",
      "path": "packs/equipment",
      "type": "Item",
      "system": "arianrhod2e"
    },
    {
      "name": "enemy-skills",
      "label": "Enemy Skills / エネミースキル",
      "path": "packs/enemy-skills",
      "type": "Item",
      "system": "arianrhod2e"
    }
  ],
  "languages": [
    {
      "lang": "ko",
      "name": "한국어",
      "path": "lang/ko.json"
    },
    {
      "lang": "ja",
      "name": "日本語",
      "path": "lang/ja.json"
    },
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "documentTypes": {
    "Actor": {
      "character": {
        "htmlFields": ["biography"]
      },
      "enemy": {
        "htmlFields": ["description"]
      },
      "guild": {
        "htmlFields": ["description"]
      }
    },
    "Item": {
      "weapon": {
        "htmlFields": ["description"]
      },
      "armor": {
        "htmlFields": ["description"]
      },
      "accessory": {
        "htmlFields": ["description"]
      },
      "skill": {
        "htmlFields": ["description"]
      },
      "item": {
        "htmlFields": ["description"]
      },
      "trap": {
        "htmlFields": ["description"]
      }
    }
  },
  "grid": {
    "distance": 1,
    "units": "sq"
  },
  "initiative": "@combat.initiative",
  "primaryTokenAttribute": "combat.hp",
  "secondaryTokenAttribute": "combat.mp",
  "socket": false,
  "url": "",
  "manifest": "",
  "download": ""
}
